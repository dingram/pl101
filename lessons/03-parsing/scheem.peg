start =
    expression

expression =
    atom
    / '(' [ \t\n\r]* a:atomlist [ \t\n\r]* ')'
    	{ return a; }

atomlist =
    a:expression [ \t\n\r]+ al:atomlist
    	{ return [a].concat(al); }
	/ a:expression
    	{ return [a]; }

validchar
    = [0-9a-zA-Z_?!+\=@#$%^&*/.-]

atom =
    chars:validchar+
        { return chars.join(""); }
