start =
    expression

expression =
    atom
    / '(' a:atomlist ')'
    	{ return a; }

atomlist =
    a:expression ' ' al:atomlist
    	{ return [a].concat(al); }
	/ a:expression
    	{ return [a]; }

validchar
    = [0-9a-zA-Z_?!+\=@#$%^&*/.-]

atom =
    chars:validchar+
        { return chars.join(""); }
